{
    "compilerOptions": {
        "noImplicitAny": false,
        "noImplicitThis": true,
        "noEmitOnError": true,
        "removeComments": false,
        "sourceMap": false,
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "types": ["vite/client"],
        "lib": ["es5", "es6", "dom", "dom.iterable", "es2016"],
        "paths": {
            "@/*": ["./resources/js/*"]
        },
    },
    "include": ["resources/js/**/*"],
}
